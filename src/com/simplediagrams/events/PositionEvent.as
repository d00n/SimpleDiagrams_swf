package com.simplediagrams.events
{
	import flash.events.Event;
	
	public class PositionEvent extends Event
	{
		public static const MOVE_TO_BACK:String = "moveToBack"
		public static const MOVE_BACKWARD:String = "moveBackward"
		public static const MOVE_FORWARD:String = "moveForward"
		public static const MOVE_TO_FRONT:String = "moveToFront"
		
		public function PositionEvent(type:String, bubbles:Boolean=false, cancelable:Boolean=false)
		{
			super(type, bubbles, cancelable);
		}
	}
}