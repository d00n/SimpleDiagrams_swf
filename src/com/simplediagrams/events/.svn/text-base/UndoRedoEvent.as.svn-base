package com.simplediagrams.events
{
	import flash.events.Event;
	
	public class UndoRedoEvent extends Event
	{
		
		public static const UNDO:String = "undoEvent"
		public static const REDO:String = "redoEvent"
					
		public function UndoRedoEvent(type:String, bubbles:Boolean=false, cancelable:Boolean=false)
		{
			super(type, bubbles, cancelable);
		}
	}
}